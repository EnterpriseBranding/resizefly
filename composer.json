{
    "name": "alpipego/resizefly",
    "description": "Dynamically resize WordPress images on the fly",
    "license": "MIT",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "Alpipego\\Resizefly\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "pimple/pimple": "^3.1.0",
        "psr/container": "^1.0",
        "spryker/code-sniffer": "^0.13.0",
        "phpstan/phpstan": "^0.10.6",
        "wapmorgan/php-code-analyzer": "^1.0",
        "sebastian/phpcpd": "^4.1"
    }
}
